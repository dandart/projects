mixin css(href)
	link(rel='stylesheet',type='text/css',href=href)
mixin js(src)
	script(type='text/javascript',src=src)
mixin jq
	+js('http://code.jquery.com/jquery-2.0.3.min.js')
mixin ko
	+js('http://knockoutjs.com/downloads/knockout-3.0.0.js')
mixin bs
	+css('//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css')
	+js('//netdna.bootstrapcdn.com/bootstrap/3.0.2/js/bootstrap.min.js')